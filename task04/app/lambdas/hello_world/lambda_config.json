{
  "version": "1.0",
  "name": "hello_world",
  "func_name": "lambdas/hello_world/index.handler",
  "resource_type": "lambda",
  "iam_role_name": "hello_world-role",
  "runtime": "nodejs20.x",
  "memory": 128,
  "timeout": 100,
  "lambda_path": "lambdas\\hello_world",
  "dependencies": [],
  "event_sources": [ {
    "resource_type": "sqs_trigger",
    "target_queue": "async_queue",
    "batch_size": 10
  },
    {
      "resource_type": "sns_topic_trigger",
      "target_topic": "lambda_topic",
      "region": "${region}"
    }],
  "env_variables": {},
  "publish_version": true,
  "alias": "${lambdas_alias_name}",
  "url_config": {"auth_type" :  "NONE"},
  "ephemeral_storage": 512,
  "tags": {}
}
